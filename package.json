{
  "name": "repomix-cron-sidequest",
  "version": "1.0.0",
  "description": "Automated job management with sidequest server - repomix runner and documentation enhancement pipeline",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "node --test tests/unit/**/*.test.js",
    "test:integration": "node --test tests/integration/*.js",
    "test:all": "node --test tests/unit/**/*.test.js tests/integration/*.js",
    "test:single": "node tests/scripts/test-single-job.js",
    "test:scanner": "node tests/scripts/test-directory-scanner.js",
    "docs:enhance": "node doc-enhancement-pipeline.js",
    "docs:enhance:dry": "node doc-enhancement-pipeline.js --dry-run",
    "docs:test": "node tests/scripts/test-single-enhancement.js",
    "git:weekly": "node git-activity-pipeline.js --weekly",
    "git:monthly": "node git-activity-pipeline.js --monthly",
    "git:schedule": "node git-activity-pipeline.js",
    "plugin:audit": "RUN_ON_STARTUP=true node plugin-management-pipeline.js",
    "plugin:audit:detailed": "DETAILED=true RUN_ON_STARTUP=true node plugin-management-pipeline.js",
    "plugin:schedule": "node plugin-management-pipeline.js",
    "claude:health": "zsh -c 'source ~/.zshrc && RUN_ON_STARTUP=true node claude-health-pipeline.js'",
    "claude:health:detailed": "zsh -c 'source ~/.zshrc && DETAILED=true RUN_ON_STARTUP=true node claude-health-pipeline.js'",
    "claude:health:schedule": "zsh -c 'source ~/.zshrc && node claude-health-pipeline.js --cron'",
    "claude:health:quick": "zsh -c 'source ~/.zshrc && SKIP_PERFORMANCE=true SKIP_PLUGINS=true RUN_ON_STARTUP=true node claude-health-pipeline.js'",
    "setup:sentry": "node setup-files/setup-sentry.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "repomix",
    "cron",
    "sidequest",
    "sentry",
    "documentation",
    "schema.org",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.120.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "node-cron": "^3.0.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "chai": "^4.3.10",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3"
  }
}
