{
  "name": "repomix-cron-sidequest",
  "version": "1.0.0",
  "description": "Automated job management with sidequest server - repomix runner and documentation enhancement pipeline",
  "main": "index.js",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "node index.js",
    "start:api": "node api/server.js",
    "dev": "node --watch index.js",
    "dashboard": "doppler run -- node api/server.js",
    "fix:types": "node scripts/fix-types.js",
    "test": "node --test --test-timeout=30000 --test-force-exit --test-name-pattern='.' tests/unit/*.test.js",
    "test:coverage": "c8 --reporter=text --reporter=text-summary node --test --test-timeout=30000 --test-force-exit --test-name-pattern='.' tests/unit/*.test.js",
    "test:coverage:html": "c8 --reporter=html --reporter=text-summary node --test --test-timeout=30000 --test-force-exit --test-name-pattern='.' tests/unit/*.test.js",
    "test:integration": "node --test --test-timeout=60000 --test-force-exit tests/integration/*.test.js",
    "test:cache": "node --test --test-timeout=60000 --test-force-exit tests/integration/cache-redis.test.js",
    "test:all": "node --test --test-timeout=30000 --test-force-exit tests/unit/**/*.test.js tests/integration/*.test.js",
    "test:validate-paths": "node tests/scripts/validate-test-paths.js",
    "test:single": "node tests/scripts/test-single-job.js",
    "test:scanner": "node tests/scripts/test-directory-scanner.js",
    "test:gitignore": "node tests/scripts/test-gitignore-respect.js",
    "test:repomix-fix": "node tests/scripts/test-repomix-fix.js",
    "docs:enhance": "RUN_ON_STARTUP=true doppler run -- node sidequest/pipeline-runners/schema-enhancement-pipeline.js",
    "docs:enhance:dry": "RUN_ON_STARTUP=true doppler run -- node sidequest/pipeline-runners/schema-enhancement-pipeline.js --dry-run",
    "docs:enhance:schedule": "doppler run -- node sidequest/pipeline-runners/schema-enhancement-pipeline.js",
    "docs:test": "node tests/scripts/test-single-enhancement.js",
    "git:weekly": "node sidequest/pipeline-runners/git-activity-pipeline.js --weekly",
    "git:monthly": "node sidequest/pipeline-runners/git-activity-pipeline.js --monthly",
    "git:schedule": "node sidequest/pipeline-runners/git-activity-pipeline.js",
    "plugin:audit": "RUN_ON_STARTUP=true node sidequest/pipeline-runners/plugin-management-pipeline.js",
    "plugin:audit:detailed": "DETAILED=true RUN_ON_STARTUP=true node sidequest/pipeline-runners/plugin-management-pipeline.js",
    "plugin:schedule": "node sidequest/pipeline-runners/plugin-management-pipeline.js",
    "claude:health": "zsh -c 'source ~/.zshrc && RUN_ON_STARTUP=true node sidequest/pipeline-runners/claude-health-pipeline.js'",
    "claude:health:detailed": "zsh -c 'source ~/.zshrc && DETAILED=true RUN_ON_STARTUP=true node sidequest/pipeline-runners/claude-health-pipeline.js'",
    "claude:health:schedule": "zsh -c 'source ~/.zshrc && node sidequest/pipeline-runners/claude-health-pipeline.js --cron'",
    "claude:health:quick": "zsh -c 'source ~/.zshrc && SKIP_PERFORMANCE=true SKIP_PLUGINS=true RUN_ON_STARTUP=true node sidequest/pipeline-runners/claude-health-pipeline.js'",
    "gitignore:update": "RUN_ON_STARTUP=true doppler run -- node sidequest/pipeline-runners/gitignore-pipeline.js",
    "gitignore:update:dry": "GITIGNORE_DRY_RUN=true RUN_ON_STARTUP=true doppler run -- node sidequest/pipeline-runners/gitignore-pipeline.js",
    "gitignore:schedule": "doppler run -- node sidequest/pipeline-runners/gitignore-pipeline.js",
    "cleanup:once": "RUN_ON_STARTUP=true doppler run -- node sidequest/pipeline-runners/repo-cleanup-pipeline.js",
    "cleanup:dryrun": "CLEANUP_DRY_RUN=true RUN_ON_STARTUP=true doppler run -- node sidequest/pipeline-runners/repo-cleanup-pipeline.js",
    "cleanup:schedule": "doppler run -- node sidequest/pipeline-runners/repo-cleanup-pipeline.js",
    "setup:sentry": "node setup-files/setup-sentry.js",
    "verify": "node scripts/verify-setup.js",
    "typecheck": "tsc --noEmit",
    "sync:cloudflare": "node scripts/sync-cloudflare-tunnel-files.js",
    "sync:cloudflare:check": "node scripts/sync-cloudflare-tunnel-files.js --check-only",
    "logs:cleanup": "node scripts/cleanup-error-logs.js",
    "logs:cleanup:dry": "node scripts/cleanup-error-logs.js --dry-run",
    "logs:cleanup:verbose": "node scripts/cleanup-error-logs.js --verbose",
    "permissions:check": "node scripts/validate-permissions.js --check-only",
    "permissions:fix": "node scripts/validate-permissions.js --fix",
    "prestart": "node scripts/validate-permissions.js --fix",
    "verify:bugfixes": "./scripts/verify-bugfixes.sh",
    "verify:bugfixes:pre": "./scripts/verify-bugfixes.sh --pre",
    "verify:bugfixes:post": "./scripts/verify-bugfixes.sh --post",
    "verify:bugfixes:health": "./scripts/verify-bugfixes.sh --health",
    "verify:bugfixes:smoke": "./scripts/verify-bugfixes.sh --smoke",
    "bugfix:once": "doppler run -- node sidequest/pipeline-runners/bugfix-audit-pipeline.js --run-now",
    "bugfix:schedule": "doppler run -- node sidequest/pipeline-runners/bugfix-audit-pipeline.js --recurring",
    "bugfix:tonight": "doppler run -- node sidequest/pipeline-runners/bugfix-audit-pipeline.js --once"
  },
  "keywords": [
    "repomix",
    "cron",
    "sidequest",
    "sentry",
    "documentation",
    "schema.org",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.29.0",
    "@babel/generator": "^7.29.1",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@isaacs/brace-expansion": "^5.0.1",
    "@sentry/node": "^7.120.4",
    "@shared/logging": "workspace:*",
    "@shared/process-io": "workspace:*",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.6",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsdom": "^27.4.0",
    "marked": "^17.0.1",
    "node-cron": "^3.0.3",
    "pino": "^9.14.0",
    "pino-pretty": "^11.3.0",
    "repomix": "^1.11.1",
    "sql.js": "^1.13.0",
    "ws": "^8.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/parser": "^7.29.0",
    "@sentry/vite-plugin": "^4.9.0",
    "@types/node": "^20.19.33",
    "c8": "^10.1.3",
    "chai": "^4.5.0",
    "glob": "^13.0.1",
    "js-yaml": "^4.1.1",
    "supertest": "^7.2.2",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@ast-grep/cli": "^0.40.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ],
    "overrides": {
      "qs": ">=6.14.1"
    }
  }
}
